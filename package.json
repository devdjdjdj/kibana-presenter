{
  "name": "kibana-presenter",
  "version": "1.0.0",
  "description": "Presenter for Kibana",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
