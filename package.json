    "morgan": "^1.10.0",
    "next": "^9.3.6",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "prettier": "^2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devdjdjdj/kibana-presenter.git"
  },
  "keywords": [
    "kibana",
    "dashboard",
